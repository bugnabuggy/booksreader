{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@br/env/*": ["src/environments/*"],
      "@br/config": ["src/app/config"],
      "@br/config/*": ["src/app/config/*"],
      "@br/public/*": ["src/app/modules/public/*"],
      "@br/reading/*": ["src/app/modules/reading/*"],
      "@br/material/*": ["src/app/modules/material/*"],
      "@br/utilities/*": ["src/app/utilities/*"],
      "@br/controls/*": ["src/app/modules/controls/*"],
      "@br/shared/*": ["src/app/modules/shared/*"],
      "@br/core/*": ["src/app/modules/core/*"],
      "@br/test/*": ["src/tests/*"],
      "@br/integrations/*": ["src/app/modules/integrations/*"]
    }
  }
}
